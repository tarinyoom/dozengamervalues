{
  "name": "dozengamervalues",
  "version": "0.0.1 'Gamerization'",
  "description": "DozenGamerValues is a gamer test that aims to map differing gamer perspectives onto six axes.",
  "main": "index.html",
  "scripts": {
    "build": "tsc && node tools/render.js && sass src:dist",
    "release": "tsc && sass src:dist --style compressed --no-source-map && node tools/inline-builder.js",
    "clean": "node tools/clean.js",
    "tsc_watch": "tsc --watch",
    "sass_watch": "sass src:dist -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QUARK88/dozenvalues.git"
  },
  "keywords": [
    "dozenvalues"
  ],
  "author": "QUARK88",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/QUARK88/dozenvalues/issues"
  },
  "homepage": "https://github.com/QUARK88/dozenvalues#readme",
  "devDependencies": {
    "pug": "^3.0.2",
    "sass": "^1.53.0",
    "terser": "^5.14.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "tsc": "^2.0.4"
  }
}
